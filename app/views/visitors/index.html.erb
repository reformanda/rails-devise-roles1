


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-8 col-lg-offset-1">
        <h2>USAASC Virtual Boards</h2>

    </div>
    <div class="col-lg-2">
    <div class="title-action">
        <%= link_to new_user_session_path do %><i class="fa fa-login"></i> Login<% end %>
    </div>
    </div>
</div>

    <div class="row">
        <div class="col-lg-10 col-lg-offset-1">
            <div class="ibox float-e-margins" style="padding: 0px;">
              <div class="ibox-title">
                  <h4>Nominations</h4>
              </div>
              <div class="ibox-content">
                <ul>
                  <% @nomination_types.find_each do |n|  %>
                  <% cont=n.code.downcase + "s"%>
                  <li>
                    <a href="<%= url_for :controller => "#{cont}", :action => 'new' %>">
                      <%= n.code %>
                    </a>
                  </li>
                  <% end %>
                </ul>
              </div>
        </div>
    </div>
    </div>
