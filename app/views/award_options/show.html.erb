<div class="row  white-bg page-heading">
    <div class="col-lg-8 col-lg-offset-1">
        <%= render "shared/usaasc_vb" %>
        <ol class="breadcrumb">
            <li>
                <a href="<%= url_for :controller => 'landing', :action => 'index' %>">Home</a>
            </li>
            <li>
                <a href="<%= url_for :controller => 'award_options', :action => 'index' %>">Award Options</a>
            </li>
        <li class="active">
            <strong>Show</strong>
        </li>
        </ol>
    </div>
    <div class="col-lg-2">
    <div class="title-action">
        <%= link_to '/users/sign_out', :method => :delete do %><i class="fa fa-sign-out"></i> Signoff<% end %>
    </div>
    </div>
</div>

<div class="row page-heading">
</div>

<div class="row">
    <div class="col-lg-10 col-lg-offset-1">
      <p id="notice"><%= notice %></p>
    </div>
</div>

<div class="row">
    <div class="col-lg-10 col-lg-offset-1">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Show Award Options</h5>
            </div>
            <div class="ibox-content">

              <p>
                <strong>Name:</strong>
                <%= @award_option.name %>
              </p>

              <p>
                <strong>Nomination type:</strong>
                <%= @award_option.nomination_type.code %>
              </p>

            </div>
        </div>
    </div>
</div>
