<div class="row  white-bg page-heading">
    <div class="col-lg-8 col-lg-offset-1">
        <%= render "shared/usaasc_vb" %>
        <ol class="breadcrumb">
            <li>
                <a href="<%= url_for :controller => 'landing', :action => 'index' %>">Home</a>
            </li>
            <li class="active">
            <strong>Award Options</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">
    <div class="title-action">
        <%= link_to '/users/sign_out', :method => :delete do %><i class="fa fa-sign-out"></i> Signoff<% end %>
    </div>
    </div>
</div>

<div class="row page-heading">
</div>

<div class="row">
    <div class="col-lg-10 col-lg-offset-1">
      <p id="notice"><%= notice %></p>
    </div>
</div>



<div class="row">
<div class="col-lg-10 col-lg-offset-1">
<div class="ibox float-e-margins">
<div class="ibox-title">
    <h5>Award Options</h5>
    <div class="title-action">
<%= link_to 'New Award Option', new_award_option_path %>
    </div>
</div>
<div class="ibox-content">

<table class="table table-striped table-bordered table-hover dataTables-example">
  <thead>
    <tr>
      <th>Name</th>
      <th>Nomintation Type</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @award_options.each do |award_option| %>
      <tr>
        <td><%= award_option.name %></td>
        <td><%= award_option.nomination_type.code %></td>
        <td><%= link_to 'Show', award_option %>
        <%= link_to 'Edit', edit_award_option_path(award_option) %>
        <%= link_to 'Destroy', award_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>
</div>
